{
  "id": "cpu_dashboard",
  "uid": "cpu_dashboard",
  "title": "CPU Dashboard",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "auto",
  "time": {
    "from": "now-5m",
    "to": "now"
  },
  "panels": [
    {
      "type": "stat",
      "title": "Avg Resident Memory: Express.js",
      "id": 1,
      "datasource": { "type": "prometheus", "uid": "prom_datasource"},
      "targets": [
        { 
          "expr": "avg(process_resident_memory_bytes{job=\"express\"}) / 1024 / 1024",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "mb",
          "color": {
            "mode": "fixed",
            "fixedColor": "green"
          }
        },
        "overrides": []
      },  
      "options": {
        "graphMode": "none"
      },
      "gridPos": { "h": 3, "w": 6, "x": 0, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Avg Resident Memory: FastAPI",
      "id": 1,
      "datasource": { "type": "prometheus", "uid": "prom_datasource"},
      "targets": [
        { 
          "expr": "avg(process_resident_memory_bytes{job=\"fastapi\"}) / 1024 / 1024",
          "legendFormat": "FastAPI",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "mb",
          "color": {
            "mode": "fixed",
            "fixedColor": "yellow"
          }
        },
        "overrides": []
      },  
      "options": {
        "graphMode": "none"
      },
      "gridPos": { "h": 3, "w": 6, "x": 6, "y": 0 }
    },
    
    {
      "type": "stat",
      "title": "CPU Usage: Express.js",
      "id": 1,
      "datasource": { "type": "prometheus", "uid": "prom_datasource"},
      "targets": [
        { 
          "expr": "process_cpu_seconds_total{job=\"express\"}",
          "legendFormat": "Express.js",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "seconds",
          "color": {
            "mode": "fixed",
            "fixedColor": "green"
          }
        },
        "overrides": []
      },  
      "options": {
        "graphMode": "none"
      }, 
      "gridPos": { "h": 3, "w": 6, "x": 12, "y": 0 }
    },
    {
      "type": "stat",
      "title": "CPU usage: FastAPI",
      "id": 1,
      "datasource": { "type": "prometheus", "uid": "prom_datasource"},
      "targets": [
        { 
          "expr": "process_cpu_seconds_total{job=\"fastapi\"}",
          "legendFormat": "FastAPI",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "seconds",
          "color": {
            "mode": "fixed",
            "fixedColor": "yellow"
          }
        },
        "overrides": []
      },  
      "options": {
        "graphMode": "none"
      }, 
      "gridPos": { "h": 3, "w": 6, "x": 18, "y": 0 }
    },
    {
  "type": "timeseries",
  "title": "Process Resident Memory Bytes ",
  "id": 7,
  "datasource": { "type": "prometheus", "uid": "prom_datasource" },
  "targets": [
    {
      "expr": "process_resident_memory_bytes{job=\"express\"} / 1024 / 1024",
      "legendFormat": " Express.js",
      "refId": "A"
    },
    {
      "expr": "process_resident_memory_bytes{job=\"fastapi\"} / 1024 / 1024",
      "legendFormat": "FastAPI",
      "refId": "B"
    }
    
  ],
  "fieldConfig": {
    "defaults": {
      "unit": "mb",
      "color": { "mode": "palette-classic" }
    },
    "overrides": []
  },
  "gridPos": { "h": 9, "w": 24, "x": 12, "y": 12 }
},
   
    {
    "type": "timeseries",
    "title": "CPU vs Virtual Users",
    "gridPos": { "h": 9, "w": 24, "x": 0, "y": 6 },

    "targets": [
        {
        "expr": "rate(process_cpu_seconds_total{job=\"express\"}[1m]) * 100",
        "legendFormat": "CPU (%) Express.js",
        "refId": "A",
        "color": "green"
        },
        {
        "expr": "rate(process_cpu_seconds_total{job=\"fastapi\"}[1m]) * 100",
        "legendFormat": "CPU (%) FastAPI",
        "refId": "B",
        "color": "yellow"
        },
        {
        "expr": "k6_vus",
        "legendFormat": "Virtual Users",
        "refId": "C",
        "color": "blue"
        }
    ],
    "fieldConfig": {
        "defaults": {
        "custom": {
            "drawStyle": "lines",
            "lineWidth": 2,
            "showPoints": "auto",
            "spanNulls": false
        },
        "unit": "percent"
        },
        "overrides": [
        {
            "matcher": {
            "id": "byName",
            "options": "Virtual Users"
            },
            "properties": [
            {
                "id": "unit",
                "value": "none"
            },
            {
                "id": "custom.axisPlacement",
                "value": "right"
            },
            {
                "id": "custom.axisLabel",
                "value": "VUs"
            },
            {
                "id": "color",
                "value": {
                  "fixedColor": "blue",
                  "mode": "fixed"
                }
            }
            ]
        },
        {
            "matcher": {
            "id": "byName",
            "options": "CPU (%) FastAPI"
            },
            "properties": [
              {
                  "id": "color",
                  "value": {
                    "fixedColor": "yellow",
                    "mode": "fixed"
                  }
              }
            ]
        },
        {
            "matcher": {
            "id": "byName",
            "options": "CPU (%) Express.js"
            },
            "properties": [
              {
                  "id": "color",
                  "value": {
                    "fixedColor": "green",
                    "mode": "fixed"
                  }
              }
            ]
        }
      ]
    },
    "options": {
        "legend": {
        "displayMode": "table",
        "placement": "bottom"
    }
  }
}



  ],
  "templating": { "list": [] }
}
