{
  "id": null,
  "title": "CPU Dashboard",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "auto",
  "time": {
    "from": "now-5m",
    "to": "now"
  },
  "panels": [
    {
      "type": "stat",
      "title": "rate(k6_http_reqs[1m])",
      "id": 1,
      "datasource": { "type": "prometheus", "uid": "prom_datasource"},
      "targets": [
        { 
          "expr": "rate(k6_http_reqs[1m])",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "k6_http_reqs_total"
        },
        "overrides": []
      },  
      "gridPos": { "h": 3, "w": 6, "x": 0, "y": 0 }
    },
    {
  "type": "timeseries",
  "title": "Avg Request Duration (Node & Python)",
  "id": 7,
  "datasource": { "type": "prometheus", "uid": "prom_datasource" },
  "targets": [
    {
      "expr": "process_resident_memory_bytes{job=\"be_node\"} / 1024 / 1024",
      "legendFormat": "Node.js avg",
      "refId": "A"
    },
    {
      "expr": "process_resident_memory_bytes{job=\"be_python\"} / 1024 / 1024",
      "legendFormat": "Python",
      "refId": "B"
    }
  ],
  "fieldConfig": {
    "defaults": {
      "unit": "megabytes",
      "color": { "mode": "palette-classic" }
    },
    "overrides": []
  },
  "gridPos": { "h": 9, "w": 12, "x": 12, "y": 12 }
},

    {
    "type": "timeseries",
    "title": "CPU vs Virtual Users",
    "gridPos": { "h": 9, "w": 24, "x": 0, "y": 6 },

    "targets": [
        {
        "expr": "rate(process_cpu_seconds_total[1m]) * 100",
        "legendFormat": "CPU (%)",
        "refId": "A"
        },
        {
        "expr": "k6_vus",
        "legendFormat": "Virtual Users",
        "refId": "B"
        }
    ],
    "fieldConfig": {
        "defaults": {
        "custom": {
            "drawStyle": "lines",
            "lineWidth": 2,
            "showPoints": "auto",
            "spanNulls": false
        },
        "unit": "percent"
        },
        "overrides": [
        {
            "matcher": {
            "id": "byName",
            "options": "Virtual Users"
            },
            "properties": [
            {
                "id": "unit",
                "value": "none"
            },
            {
                "id": "custom.axisPlacement",
                "value": "right"
            },
            {
                "id": "custom.axisLabel",
                "value": "VUs"
            }
            ]
        }]
    },
    "options": {
        "legend": {
        "displayMode": "table",
        "placement": "bottom"
    }
  }
}



  ],
  "templating": { "list": [] }
}
